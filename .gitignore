cat > .gitignore << 'EOF'
# Virtual environment
venv/
env/
*.pyc
__pycache__/

# Large data files - don't upload to GitHub
data/raw/*.pdf
data/processed/documents.json
data/processed/wikipedia_compliance.json
data/processed/chunks.json
data/processed/synthetic_qa_*.json

# ChromaDB database - IMPORTANT: exclude entire directory
chroma_db/

# OS files
.DS_Store
*.swp
._.DS_Store

# Ollama models (too large)
*.gguf

# IDE
.vscode/
.idea/

# Logs
*.log
EOF